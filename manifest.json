{
    "manifest_version": 3,
    "name": "Chrome Tab",
    "version": "0.1",
    "description": "Chrome extension that replicates various Safari behaviours.",
    "author": "submarines",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks"
    ],
    "icons": {
        "16": "icons/16.png",
        "19": "icons/19.png",
        "38": "icons/38.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}